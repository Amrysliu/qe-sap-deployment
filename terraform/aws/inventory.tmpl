all:
  children:
    hana:
      hosts:
%{ for index, value in hana-pip ~}
        ${hana_hostname}${format("%02d", index + 1)}:
          ansible_host: ${value}
          ansible_python_interpreter: /usr/bin/python3
%{ endfor ~}
 %{ if iscsi-enabled }
    iscsi:
      hosts:
 %{ for index, value in iscsi-pip ~}
        ${iscsi_hostname}${format("%02d", index + 1)}:
          ansible_host: ${value}
          ansible_python_interpreter: /usr/bin/python3
%{ endfor ~}
%{ endif }
  hosts: null